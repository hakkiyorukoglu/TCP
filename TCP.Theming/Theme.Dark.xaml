<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--
        Theme.Dark.xaml - Koyu tema tanımları
        
        Bu dosya koyu tema için özel renk değerlerini içerir.
        ThemeTokens.xaml'deki token'ları override ederek koyu tema renklerini sağlar.
        
        Şu an için sadece koyu tema mevcut, gelecekte açık tema eklenebilir.
    -->
    
    <ResourceDictionary.MergedDictionaries>
        <!-- Önce token'ları yükle -->
        <ResourceDictionary Source="ThemeTokens.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <!-- Koyu tema için özel renk override'ları (şu an için gerekli değil, 
         token'lar zaten koyu tema için tanımlı) -->
    
</ResourceDictionary>
